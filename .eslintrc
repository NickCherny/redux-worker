{
    "parser": "babel-eslint",
    "rules": {
        "object-curly-newline": 1,
        "no-console": 0,
        "no-debugger": 1,
        "indent": [2, 2, {
            "SwitchCase": 1,
            "FunctionDeclaration": { "parameters": 1, "body": 1 },
            "FunctionExpression": { "parameters": 1, "body": 1 }
        }],
        "camelcase": [2, {"properties": "always"}],
        "max-statements": [2, 32],
        "complexity": [2, 20],
        "max-depth": [2],
        "max-len": [2, {
            "code": 100,
            "ignoreTemplateLiterals": true,
            "ignoreStrings": true,
            "ignoreComments": true
        }],
        "max-params": [2, 5],
        "max-lines": [2, {
            "skipBlankLines": true,
            "skipComments": true
        }],
        "template-tag-spacing": [0, "never"],
        "wrap-iife": [2, "inside"],
        "new-cap": [1, { "capIsNewExceptions": [] }],
        "no-invalid-this": 2,
        "global-require": 0,
        "comma-dangle": [0, "never"],
        "no-plusplus": 0,
        "no-return-assign": 0,
        "class-methods-use-this": 0,
        "function-paren-newline": 1,
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved": 0,
        "import/extensions": 0,
        "jsx-a11y/no-static-element-interactions": 0,
        "jsx-a11y/label-has-for": [ 0, {
            "components": [ "Label" ]
        }]
    },

    "env": {
        "es6": true,
        "browser": true,
        "commonjs": true,
        "mocha": true
    },

    "extends": [],

    "globals": {
        "google": true,
        "self": true
    }
}
